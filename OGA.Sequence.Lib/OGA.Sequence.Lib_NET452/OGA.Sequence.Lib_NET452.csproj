<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <Configurations>Debug;Release</Configurations>
    <Version>1.0.1</Version>
    <AssemblyVersion>1.0.1.1</AssemblyVersion>
    <FileVersion>1.0.1.1</FileVersion>
    <Company>OGA</Company>
    <Product>OGA Libraries</Product>
    <PackageId>OGA.Sequence.Lib</PackageId>
    <Authors>Lee White</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Description>OGA Sequence Library</Description>
    <PackageReleaseNotes>(Please write the package release notes in "OGA.Sequence.Lib-RELEASE-NOTES.txt".)</PackageReleaseNotes>
    <AssemblyName>OGA.Sequence.Lib</AssemblyName>
    <RootNamespace>OGA.Sequence.Lib</RootNamespace>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);NET452</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);NET452</DefineConstants>
  </PropertyGroup>

  <Import Project="..\OGA.Sequence.Lib_SP\OGA.Sequence.Lib_SP.projitems" Label="Shared" />

  <ItemGroup>
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>

</Project>
